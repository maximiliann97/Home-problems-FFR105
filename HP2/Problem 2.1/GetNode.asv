    numberOfCities = length(visibility);

    for i = 1:numberOfCities
        if ~ismember(i,tabuList)
            probability = StepProbability(pheromoneLevel,visibility,tabuList,alpha,beta,startingNode,i);
        else
            continue
        end
    end

end

